+OPDEF(nearskip,  1, 0) // must be skip-1 and lsb=1
+OPDEF(skip,      2, 0)
+OPDEF(neartramp, 3, 0) // must be tramp-1 and lsb=1
+OPDEF(tramp,     4, 0)
+
+OPDEF(unused1,   5, 2)
+OPDEF(unused2,   6, 2)
+OPDEF(unused3,   7, 2)
