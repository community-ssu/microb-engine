+void nsAudioStream::Drain()
+{
+  if (!mAudioHandle) {
+    PRUint32 drainTime = (float(mSamplesBuffered) / mRate / mChannels - GetTime()) * 1000.0;
+    PR_Sleep(PR_MillisecondsToInterval(drainTime));
+    return;
