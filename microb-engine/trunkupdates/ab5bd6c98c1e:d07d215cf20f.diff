-{
-}
-
-NPBool nsPluginInstance::init(NPWindow* aWindow)
-{
-  if(aWindow == NULL)
-    return FALSE;
-  
-  mWindow = aWindow;
-  mInitialized = TRUE;
-  mSaveClip = NewRgn();
-  return TRUE;
-}
