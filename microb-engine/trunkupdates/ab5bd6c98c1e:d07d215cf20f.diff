-/////////////////////////////////////////////////////////////
-//
-// HandleEvent
-//
-uint16_t
-nsPluginInstance::HandleEvent(void* event)
-{
-	int16_t eventHandled = FALSE;
-	
-	EventRecord* ev = (EventRecord*) event;
-	if (event != NULL)
-	{
-		switch (ev->what)
-		{
-			/*
-			 * Draw ourselves on update events
-			 */
-			case updateEvt:
-				if( StartDraw(mWindow) ) {
-					DoDraw();
-					EndDraw(mWindow);
-				}
-				eventHandled = true;
-				break;
-
-			default:
-				break;
-		}
-	}
-	return eventHandled;
-}
