-//
-// SetWindow
-//
-NPError
-nsPluginInstance::SetWindow(NPWindow* window)
-{
-	mWindow = window;
-	if( StartDraw(window) ) {
-		DoDraw();
-		EndDraw(window);
-	}
-	return NPERR_NO_ERROR;
-}
