+nsRegion
+nsDOMNotifyPaintEvent::GetRegion()
+{
+  nsNotifyPaintEvent* event = static_cast<nsNotifyPaintEvent*>(mEvent);
+
+  nsRegion r;
+  if (nsContentUtils::IsCallerTrustedForRead()) {
+    r.Or(event->sameDocRegion, event->crossDocRegion);
