+      r3.surroundContents(document.createElement("div"));
+      is(r3.toString(), "Hello ", "Wrong range!");
+    } catch(e) {
+      ex = e;
+      is(e.code, 1, "Didn't get BAD_BOUNDARYPOINTS_ERR exception!");
