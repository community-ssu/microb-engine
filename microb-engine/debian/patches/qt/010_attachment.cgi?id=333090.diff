+QtPromptService::Confirm(nsIDOMWindow* aParent,
+                         const PRUnichar* aDialogTitle,
+                         const PRUnichar* aDialogText,
+                         PRBool* aConfirm)
+{
+    return
+        ConfirmCheck(aParent,
+                     aDialogTitle, aDialogText,
+                     NULL, NULL,
+                     aConfirm);
+}
