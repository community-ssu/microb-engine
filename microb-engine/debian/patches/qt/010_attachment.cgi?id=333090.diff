+NS_IMETHODIMP
+nsFilePicker::GetDefaultExtension(nsAString & aDefaultExtension)
+{
+    aDefaultExtension = mDefaultExtension;
+
+    return NS_OK;
+}
+NS_IMETHODIMP
+nsFilePicker::SetDefaultExtension(const nsAString & aDefaultExtension)
+{
+    mDefaultExtension = aDefaultExtension;
+
+    return NS_OK;
+}
